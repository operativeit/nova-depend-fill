{
    "name": "eom-plus/nova-depend-fill",
    "description": "Fill Laravel Nova field based on value from other field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "EomPlus\\Nova\\Fields\\DependFill\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EomPlus\\Nova\\Fields\\DependFill\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
